    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug mscp (WSL)",
            "type": "WSL",
            "request": "launch",
            "program": "/mnt/d/SyncWork/CodeSpace/DTN/bondmscp/build/mscp",
            "args": [
                "-vvv", 
                "-D", 
                "eth3", 
                "-P", 
                "22", 
                "/root/zipable.tar.1", 
                "vtuser@10.0.0.210:c\\users\\vtuser\\leon1.t"
            ],
            "stopAtEntry": false,
            "cwd": "/mnt/d/SyncWork/CodeSpace/DTN/bondmscp",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/mnt/d": "d:\\\\",
                "/mnt/d/SyncWork/CodeSpace/DTN/bondmscp": "${workspaceFolder}"
            },
            "pipeTransport": {
                "pipeCwd": "/mnt/d/SyncWork/CodeSpace/DTN/bondmscp",
                "pipeProgram": "wsl",
                "pipeArgs": ["-e"],
                "debuggerPath": "/bin/gdb"
            }
        }
    ]
